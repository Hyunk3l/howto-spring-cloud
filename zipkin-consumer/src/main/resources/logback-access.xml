<configuration>
    <appender name="CONSOLE_ACCESS" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%h %l %u [%t] "%r" %s %b "%i{Referer}" "%i{User-Agent}" %b %D "%i{X-Parent-Id}" "%i{X-Trace-Id}"</pattern>
        </encoder>
    </appender>

    <appender-ref ref="CONSOLE_ACCESS" />
</configuration>


